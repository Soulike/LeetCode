{
  "name": "leet-code",
  "version": "1.0.0",
  "type": "module",
  "repository": "git@github.com:Soulike/LeetCode.git",
  "author": "Soulike <git_anonymous@foxmail.com>",
  "license": "MIT",
  "devDependencies": {
    "clang-format": "^1.8.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "prettier": "^3.5.3"
  },
  "scripts": {
    "prepare": "husky"
  },
  "prettier": {
    "tabWidth": 2,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "bracketSameLine": true
  },
  "lint-staged": {
    "**/*.{js,ts,json}": "prettier --write --ignore-unknown",
    "**/*.{cpp,h}": [
      "clang-format -i"
    ]
  }
}
